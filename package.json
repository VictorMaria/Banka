{
  "name": "Banka",
  "version": "1.0.0",
  "description": "Banka is a light-weight core banking application that powers banking operations like account creation, customer deposit and withdrawals. This app is meant to support a single bank, where users can signup and create bank accounts online, but must visit the branch to withdraw or deposit money..",
  "main": "app.js",
  "dependencies": {
    "bcrypt": "^3.0.5",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "mocha-lcov-reporter": "^1.3.0",
    "multer": "^1.4.1",
    "node": "^11.13.0",
    "nodemailer": "^6.1.0",
    "nyc": "^13.3.0",
    "supertest": "^4.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.16.0",
    "mocha": "^6.1.2",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2"
  },
  "scripts": {
    "test": "nyc --reporter=lcov --reporter=text --timeout 40000 mocha dist/server/test --exit",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VictorMaria/Banka.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/VictorMaria/Banka/issues"
  },
  "homepage": "https://github.com/VictorMaria/Banka#readme"
}
